use v6.d;
use Test;
use RakuDoc::Render;

plan 1;

my $ast;
my $rv;
my RakuDoc::Processor $rdp .= new( :test, :process );

$ast = Q:to/QAST/.AST;
    =head2 First header
    =begin section
    =head2 Second header without option
    =config head2 :!toc
    =head2 Third test text with option
    =end section
    =head2 Fourth without option
    QAST
$rv = $rdp.render( $ast, :process );
is $rv.toc.elems, 3, 'got correct number of contents';

done-testing;
